       after="org.knime.geo.concave.ConcaveHullNodeFactory"
       category-path="/geo/operations"
       factory-class="org.knime.geo.unary.UnaryUnionNodeFactory"
       id="org.knime.geo.unary.UnaryUnionNodeFactory"/>
	<node
       after="org.knime.geo.difference.DifferenceNodeFactory"
       category-path="/geo/operations"
       factory-class="org.knime.geo.combine.CombineNodeFactory"
       id="org.knime.geo.combine.CombineNodeFactory"/>
	<node
       after="org.knime.geo.unary.UnaryUnionNodeFactory"
       category-path="/geo/operations"
       factory-class="org.knime.geo.removepoints.RemoveRepPointsNodeFactory"
       id="org.knime.geo.removepoints.RemoveRepPointsNodeFactory"/>
	<node
       after="org.knime.geo.removepoints.RemoveRepPointsNodeFactory"
       category-path="/geo/operations"
       factory-class="org.knime.geo.reomovehole.RemoveHolesNodeFactory"
       id="org.knime.geo.reomovehole.RemoveHolesNodeFactory"/>
	<node
       after="org.knime.geo.reomovehole.RemoveHolesNodeFactory"
       category-path="/geo/operations"
       factory-class="org.knime.geo.mbr.MinBoundingRectNodeFactory"
       id="org.knime.geo.mbr.MinBoundingRectNodeFactory"/>
	<node category-path="/geo/gdal" factory-class="org.knime.geo.resample.ResampleNodeFactory" id="org.knime.geo.resample.ResampleNodeFactory"/>
	<node
       after="org.knime.geo.resample.ResampleNodeFactory"
       category-path="/geo/gdal"
       factory-class="org.knime.geo.mosaic.MosaicNodeFactory"
       id="org.knime.geo.mosaic.MosaicNodeFactory"/>	
	<node
       after="org.knime.geo.mosaic.MosaicNodeFactory"
       category-path="/geo/gdal"
       factory-class="org.knime.geo.mask.MaskRasterNodeFactory"
       id="org.knime.geo.mask.MaskRasterNodeFactory"/>
	<node
       after="org.knime.geo.mask.MaskRasterNodeFactory"
       category-path="/geo/gdal"
       factory-class="org.knime.geo.calc.GdalCalcNodeFactory"
       id="org.knime.geo.calc.GdalCalcNodeFactory"/>
	<node
       after="org.knime.geo.calc.GdalCalcNodeFactory"
       category-path="/geo/gdal"
       factory-class="org.knime.geo.clip.ClipPolygonToRasterNodeFactory"
       id="org.knime.geo.clip.ClipPolygonToRasterNodeFactory"/>
	<node
       after="org.knime.geo.clip.ClipPolygonToRasterNodeFactory"
       category-path="/geo/gdal"
       factory-class="org.knime.geo.rasterize.RasterizeNodeFactory"
       id="org.knime.geo.rasterize.RasterizeNodeFactory"/>
	<node
       after="org.knime.geo.rasterize.RasterizeNodeFactory"
       category-path="/geo/gdal"
       factory-class="org.knime.geo.proximity.ProximityNodeFactory"
       id="org.knime.geo.proximity.ProximityNodeFactory"/>
	<node category-path="/geo/conv" factory-class="org.knime.geo.transform.TransformNodeFactory" id="org.knime.geo.transform.TransformNodeFactory"/>
 	<node
        after="org.knime.geo.transform.TransformNodeFactory"
        category-path="/geo/conv"
        factory-class="org.knime.geo.snapper.SnapToGridNodeFactory"
        id="org.knime.geo.snapper.SnapToGridNodeFactory"/>  
	<node
       after="org.knime.geo.snapper.SnapToGridNodeFactory"
       category-path="/geo/conv"
       factory-class="org.knime.geo.polygontoline.PolygonToLineNodeFactory"
       id="org.knime.geo.polygontoline.PolygonToLineNodeFactory"/>					
	<node
       after="org.knime.geo.polygontoline.PolygonToLineNodeFactory"
       category-path="/geo/conv"
       factory-class="org.knime.geo.filter.FilterGeomNodeFactory"
       id="org.knime.geo.filter.FilterGeomNodeFactory"/>
 	<node
        after="org.knime.geo.filter.FilterGeomNodeFactory"
        category-path="/geo/conv"
        factory-class="org.knime.geo.multtoply.MultiPolygonToPolygonNodeFactory"
        id="org.knime.geo.multtoply.MultiPolygonToPolygonNodeFactory"/>
	<node category-path="/geo/info" factory-class="org.knime.geo.centroid.CentroidsNodeFactory" id="org.knime.geo.centroid.CentroidsNodeFactory"/>
	<node category-path="/geo/info" factory-class="org.knime.geo.linemerge.LineMergerNodeFactory" id="org.knime.geo.linemerge.LineMergerNodeFactory"/>
	<node category-path="/geo/info" factory-class="org.knime.geo.length.LengthNodeFactory" id="org.knime.geo.length.LengthNodeFactory"/>
	<node category-path="/geo/info" factory-class="org.knime.geo.linepoint.LinePointsNodeFactory" id="org.knime.geo.linepoint.LinePointsNodeFactory"/>
	<node category-path="/geo/info" factory-class="org.knime.geo.distance.DistanceNodeFactory" id="org.knime.geo.distance.DistanceNodeFactory"/>
	<node category-path="/geo/info" factory-class="org.knime.geo.area.AreaNodeFactory" id="org.knime.geo.area.AreaNodeFactory"/>
	<node category-path="/geo/bool" factory-class="org.knime.geo.bool.intersects.IntersectsNodeFactory" id="org.knime.geo.bool.intersects.IntersectsNodeFactory"/>
	<node category-path="/geo/bool" factory-class="org.knime.geo.bool.touches.TouchesNodeFactory" id="org.knime.geo.bool.touches.TouchesNodeFactory"/>
	<node category-path="/geo/bool" factory-class="org.knime.geo.bool.contains.ContainsNodeFactory" id="org.knime.geo.bool.contains.ContainsNodeFactory"/>
	<node category-path="/geo/bool" factory-class="org.knime.geo.bool.covers.CoversNodeFactory" id="org.knime.geo.bool.covers.CoversNodeFactory"/>
	<node category-path="/geo/bool" factory-class="org.knime.geo.bool.crosses.CrossesNodeFactory" id="org.knime.geo.bool.crosses.CrossesNodeFactory"/>
	<node category-path="/geo/bool" factory-class="org.knime.geo.bool.coveredBy.CoveredByNodeFactory" id="org.knime.geo.bool.coveredBy.CoveredByNodeFactory"/>
	<node category-path="/geo/bool" factory-class="org.knime.geo.bool.disjoint.DisjointNodeFactory" id="org.knime.geo.bool.disjoint.DisjointNodeFactory"/>
	<node category-path="/geo/bool" factory-class="org.knime.geo.bool.overlap.OverlapNodeFactory" id="org.knime.geo.bool.overlap.OverlapNodeFactory"/>
	<node category-path="/geo/bool" factory-class="org.knime.geo.bool.within.WithinNodeFactory" id="org.knime.geo.bool.within.WithinNodeFactory"/>
	<node category-path="/geo/bool" factory-class="org.knime.geo.bool.equals.EqualsNodeFactory" id="org.knime.geo.bool.equals.EqualsNodeFactory"/>	
	<node category-path="/geo/db" factory-class="org.knime.MyDBTableConn.MyDBTableConnNodeFactory" id="org.knime.MyDBTableConn.MyDBTableConnNodeFactory"/>
    <node category-path="/geo/db" factory-class="org.knime.MyDBConn.MyDBConnNodeFactory" id="org.knime.MyDBConn.MyDBConnNodeFactory"/>
    <node category-path="/geo/db" factory-class="org.knime.MyDBReader.MyDBReaderNodeFactory" id="org.knime.MyDBReader.MyDBReaderNodeFactory"/>
    <!--node category-path="/geo/db" factory-class="org.knime.MyPortDBConn.MyPortDBConnNodeFactory" id="org.knime.MyPortDBConn.MyPortDBConnNodeFactory"/-->
    <node category-path="/geo/db" factory-class="org.knime.MyDBWriter.MyDBWriterNodeFactory" id="org.knime.MyDBWriter.MyDBWriterNodeFactory"/>
</extension>
	<extension point="org.knime.core.PortType">
	      <portType color="#00FF00" hidden="false" name="MyDatabase Conn" objectClass="org.knime.mynode.myport.MyDatabaseConnPortObject" objectSerializer="org.knime.mynode.myport.MyDatabaseConnPortObject$Serializer" specClass="org.knime.mynode.myport.MyDatabaseConnPortObjectSpec" specSerializer="org.knime.mynode.myport.MyDatabaseConnPortObjectSpec$Serializer">
	      </portType>
	 </extension>
 <extension point="org.knime.workbench.repository.metanode">
    <PersistedMetaNode category-path="/geo/meta" description="Find the intersection between two geometries" expert-flag="false" icon="icons/intersections.png" id="intersects" name="Intersects" workflowDir="meta/Intersects/">
    </PersistedMetaNode>
    <PersistedMetaNode category-path="/geo/meta" expert-flag="false" icon="icons/difference.png" id="difference" name="Difference" workflowDir="meta/Difference/">
    </PersistedMetaNode>
 </extension>
   
</plugin>
