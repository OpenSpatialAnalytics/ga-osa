<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>
      <extension point="org.knime.workbench.repository.categories">
      <category description="Geo-Spatial operations of GeoTools" icon="icons/geom.png" level-id="geo" name="Geo-spatial Data Processing" path="/">
	  </category>
      <category description="Reading and writing geometry files" icon="icons/io.png" level-id="io" name="IO" path="/geo">
      </category>
      <category after="io" description="Basic Geo-Spatial operations." icon="icons/operations.png" level-id="operations" name="Geoprocesing" path="/geo">
      </category>
      <category after="gdal" description="Geometry conversion" icon="icons/conversion.png" level-id="conv" name="Conversion" path="/geo">
      </category>
      <category after="conv" description="Describe geometry" icon="icons/info.png" level-id="info" name="Geometry Info" path="/geo">
      </category>    
      <category after="info" description="Boolean operations" icon="icons/boolean.png" level-id="bool" name="Boolean Operations" path="/geo">         
      </category>
      <category after="bool" description="Geo map visualization" icon="icons/visualization.png" level-id="view" name="Map Viewer" path="/geo">
      </category>
      <category after="operations" description="GDAL raster operations" icon="icons/gdal.png" level-id="gdal" name="Raster Operations" path="/geo">         
      </category>  
       <category after="bool" description="PostGIS operations" icon="icons/database-128.png" level-id="db" name="PostGIS Operations" path="/geo">         
      </category>   
       <category after="db" description="Meta Nodes" icon="icons/meta.png" level-id="meta" name="Meta Nodes" path="/geo">         
      </category>             
   	</extension>
   
 <extension point="org.knime.workbench.repository.nodes">
    <node
          category-path="/geo/io"
          factory-class="org.knime.geo.reader.ShapeFileReaderNodeFactory"
          id="org.knime.geo.spatial.ShapeFileReaderNodeFactory"/>
    <node
          after="org.knime.geo.spatial.ShapeFileReaderNodeFactory"
          category-path="/geo/io"
          factory-class="org.knime.geo.writer.ShapeFileWriterNodeFactory"
          id="org.knime.geo.writer.ShapeFileWriterNodeFactory"/>
    <node
          after="org.knime.geo.writer.ShapeFileWriterNodeFactory"
          category-path="/geo/io"
          factory-class="org.knime.geo.shpmerge.ShapeMergeNodeFactory"
          id="org.knime.geo.shpmerge.ShapeMergeNodeFactory"/>
	<node
       after="org.knime.geo.shpmerge.ShapeMergeNodeFactory"
       category-path="/geo/io"
       factory-class="org.knime.geo.shapetojson.ShapeToGeoJsonNodeFactory"
       id="org.knime.geo.shapetojson.ShapeToGeoJsonNodeFactory"/>
	<node
       after="org.knime.geo.shapetojson.ShapeToGeoJsonNodeFactory"
       category-path="/geo/io"
       factory-class="org.knime.geo.jsonreader.GeoJsonReaderNodeFactory"
       id="org.knime.geo.jsonreader.GeoJsonReaderNodeFactory"/>
	<node
       after="org.knime.geo.jsonreader.GeoJsonReaderNodeFactory"
       category-path="/geo/io"
       factory-class="org.knime.geo.jsonwriter.GeoJsonWriterNodeFactory"
       id="org.knime.geo.jsonwriter.GeoJsonWriterNodeFactory"/>
	<node
       after="org.knime.geo.jsonwriter.GeoJsonWriterNodeFactory"
       category-path="/geo/io"
       factory-class="org.knime.geo.listhdr.ListHdrNodeFactory"
       id="org.knime.geo.listhdr.ListHdrNodeFactory"/>
	<node
       after="org.knime.geo.listhdr.ListHdrNodeFactory"
       category-path="/geo/io"
       factory-class="org.knime.geo.rank.RankFilesNodeFactory"
       id="org.knime.geo.rank.RankFilesNodeFactory"/>
	<node
       after="org.knime.geo.rank.RankFilesNodeFactory"
       category-path="/geo/io"
       factory-class="org.knime.MyWFS.MyWFSNodeFactory"
       id="org.knime.MyWFS.MyWFSNodeFactory"/>
   	<node category-path="/geo/view" factory-class="org.knime.geo.visualizer.GeoMapViewerNodeFactory" id="org.knime.geo.visualizer.GeoMapViewerNodeFactory"/>
	<node category-path="/geo/operations" factory-class="org.knime.geo.buffer.BufferNodeFactory" id="org.knime.geo.buffer.BufferNodeFactory"/>
	<node
       after="org.knime.geo.buffer.BufferNodeFactory"
       category-path="/geo/operations"
       factory-class="org.knime.geo.intersection.IntersectionNodeFactory"
       id="org.knime.geo.intersection.IntersectionNodeFactory"/>
	<node
       after="org.knime.geo.intersection.IntersectionNodeFactory"
       category-path="/geo/operations"
       factory-class="org.knime.geo.union.UnionNodeFactory"
       id="org.knime.geo.union.UnionNodeFactory"/>
	<node
       after="org.knime.geo.union.UnionNodeFactory"
       category-path="/geo/operations"
       factory-class="org.knime.geo.difference.DifferenceNodeFactory"
       id="org.knime.geo.difference.DifferenceNodeFactory"/>
	<node
       after="org.knime.geo.combine.CombineNodeFactory"
       category-path="/geo/operations"
       factory-class="org.knime.geo.split.SplitNodeFactory"
       id="org.knime.geo.split.SplitNodeFactory"/>
	<node
       after="org.knime.geo.split.SplitNodeFactory"
       category-path="/geo/operations"
       factory-class="org.knime.geo.convex.ConvexHullNodeFactory"
       id="org.knime.geo.convex.ConvexHullNodeFactory"/>
	<node
       after="org.knime.geo.convex.ConvexHullNodeFactory"
